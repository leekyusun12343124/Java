if "사자" == "사자":
    print("사자랑 사자는 같아, 실행될까?")

if "사자" == "거위":
    print("사자랑 거위는 달라, 실행될까?")

    answer_1 = "짜장면"
    answer_2 = "볶음밥"
    answer_3 = "짜장면"

    if answer_1 == answer_2:
        print("answer_1과 answer_2는 다르지, 실행될까?")

    if answer_1 == answer_3:
        print("answer_1과 answer_3는 같지, 실행될까?")

    if True:
        print("True는 참이라는 뜻이야")

    if False:
        print("False는 거짓이라는 뜻이야")
